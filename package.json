{
  "name": "electron-react-typescript-starter",
  "version": "1.0.0",
  "description": "<description>",
  "author": "<author>",
  "build": {
    "appId": "<com.electronapp>",
    "productName": "<productName>"
  },
  "main": "./public/electron.js",
  "homepage": "./",
  "private": true,
  "dependencies": {
    "electron-is-dev": "2.0.0",
    "node-sass": "5.0.0",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@types/node": "15.0.1",
    "@types/react": "17.0.4",
    "@types/react-dom": "17.0.3",
    "concurrently": "6.0.2",
    "cross-env": "7.0.3",
    "electron": "12.0.5",
    "electron-builder": "22.10.5",
    "react-scripts": "4.0.3",
    "typescript": "4.2.4",
    "wait-on": "5.3.0"
  },
  "scripts": {
    "dev": "concurrently \"cross-env BROWSER=none yarn react:start\" \"wait-on http://localhost:3000 && electron .\"",
    "electron:build": "electron-builder --publish=never",
    "electron:package": "yarn react:build && yarn electron:build",
    "react:start": "react-scripts start",
    "react:build": "react-scripts build",
    "release": "yarn react:build && electron-builder --publish=never"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version"
    ]
  }
}
